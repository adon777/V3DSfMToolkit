<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="lpsolve"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="&quot;$(lpsolve)\include&quot;"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="lpsolve55.lib liblpsolve55.lib"
		AdditionalLibraryDirectories="$(lpsolve)\lib\$(PlatformName)"
	/>
	<UserMacro
		Name="lpsolve"
		Value="$(SolutionDir)\Dependencies\lpsolve"
	/>
</VisualStudioPropertySheet>
