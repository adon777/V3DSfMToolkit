<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="libjpeg"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(libjpeg)\include"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="jpeg-static.lib"
		AdditionalLibraryDirectories="$(libjpeg)\lib\$(PlatformName)"
	/>
	<UserMacro
		Name="libjpeg"
		Value="$(SolutionDir)\Dependencies\libjpeg"
	/>
</VisualStudioPropertySheet>
